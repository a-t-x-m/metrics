    "wildcard": "^2.0.0"
  },
  "devDependencies": {
    "@types/atom": "^1.40.1",
    "@types/node": "^13.7.7",
    "husky": "^4.2.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
