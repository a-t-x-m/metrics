{
  "name": "@atxm/metrics",
  "version": "0.6.0",
  "description": "Google Analytics event tracking for Atom packages",
  "license": "MIT OR GPL-2.0",
  "scripts": {
    "build": "rimraf tsconfig.tsbuildinfo && tsc --pretty --project ./",
    "dev": "npm run start",
    "lint": "tslint --project ./",
    "lint:fix": "tslint --fix --project ./",
    "start": "tsc --watch --pretty --project ./"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-t-x-m/metrics.git"
  },
  "keywords": [
    "atom",
    "atom package"
  ],
  "dependencies": {
    "@atxm/developer-console": "^0.2.2",
    "address": "^1.1.2",
    "caller-callsite": "^4.1.0",
    "query-string": "^6.12.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@types/atom": "^1.40.4",
    "@types/node": "^14.0.1",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
